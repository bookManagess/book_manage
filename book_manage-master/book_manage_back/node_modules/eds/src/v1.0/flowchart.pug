extends ../pug/layouts/basic.pug

block sidebar
	aside.col-md-3.col-sm-3.sidebar
		ul.sidenav.dropable.sticky
			li
				a(href='#').open.active Flowchart
				ul(style="display: block;")
					div(data-edstarget="drawio" data-edsfoldername="flowchart")
						li
							a(href='#' + '{{fileName}}') {{fileName}}

block articleHeader
	ol.breadcrumb
		li
			a(href='index.html') 首頁
		li.active Flowchart

block article
	section
		h2 Flowchart
		div(data-edstarget="drawio" data-edsfoldername="flowchart")
			span {{fileName}}
			span {{file}}
			hr

block append footer
	// Scripts
	script(src='../js/nextdocs.js')
	script(src='https://www.draw.io/js/viewer.min.js')
	script(src='../js/app.js')
